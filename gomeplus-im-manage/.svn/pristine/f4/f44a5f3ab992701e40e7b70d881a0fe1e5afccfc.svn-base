package com.gomeplus.im.manage.pojo;

import java.io.Serializable;
import java.util.List;

/**
 * 店铺
 */
public class MsgShop implements Serializable {
	private static final long serialVersionUID = 1L;

	private long id; // 店铺id
	private String name; // 名称
	private List<String> imgUrl; // 店铺图片地址列表
	private String shopUrl; // 店铺url地址
	private int discountType; // 优惠类型；0:无优惠、1:红包、2:返利、......
	private String extra; // 扩展

	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public List<String> getImgUrl() {
		return imgUrl;
	}

	public void setImgUrl(List<String> imgUrl) {
		this.imgUrl = imgUrl;
	}

	public String getShopUrl() {
		return shopUrl;
	}

	public void setShopUrl(String shopUrl) {
		this.shopUrl = shopUrl;
	}

	public int getDiscountType() {
		return discountType;
	}

	public void setDiscountType(int discountType) {
		this.discountType = discountType;
	}

	public String getExtra() {
		return extra;
	}

	public void setExtra(String extra) {
		this.extra = extra;
	}

}
