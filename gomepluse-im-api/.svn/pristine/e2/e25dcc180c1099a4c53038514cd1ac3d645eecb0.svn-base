package com.gomeplus.im.api.request;

import java.io.Serializable;

/**
 * 好友分组
 */
public class ReqFriendGroup  implements Serializable {
	private long id; //id 主键
    private long userId;//用户ID
    private String groupName;  //分组的名称
    
    public ReqFriendGroup() {
	}

	public ReqFriendGroup(long id, long userId, String groupName) {
		super();
		this.id = id;
		this.userId = userId;
		this.groupName = groupName;
	}

	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}

	public long getUserId() {
		return userId;
	}

	public void setUserId(long userId) {
		this.userId = userId;
	}

	public String getGroupName() {
		return groupName;
	}

	public void setGroupName(String groupName) {
		this.groupName = groupName;
	}
    
}
