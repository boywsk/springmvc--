package com.gomeplus.im.api.message;

import java.io.Serializable;

public class DelFriendMsg implements Serializable {
	/**
	 * 删除好友
	 */
	private static final long serialVersionUID = 1L;
	private long fromUid; // 申请用户ID
	private long toUid; // 审批用户ID
	private String content3; // 内容
	private long optTime; // 操作时间(毫秒)
	private String extra; // 扩展

	public long getFromUid() {
		return fromUid;
	}

	public void setFromUid(long fromUid) {
		this.fromUid = fromUid;
	}

	public long getToUid() {
		return toUid;
	}

	public void setToUid(long toUid) {
		this.toUid = toUid;
	}

	public String getContent3() {
		return content3;
	}

	public void setContent3(String content3) {
		this.content3 = content3;
	}

	public long getOptTime() {
		return optTime;
	}

	public void setOptTime(long optTime) {
		this.optTime = optTime;
	}

	public String getExtra() {
		return extra;
	}

	public void setExtra(String extra) {
		this.extra = extra;
	}

}
